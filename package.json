{
  "name": "@nodenv/nodenv-each",
  "version": "1.2.0",
  "description": "nodenv plugin to run a command across all installed nodes",
  "homepage": "https://github.com/nodenv/nodenv-each#readme",
  "license": "MIT",
  "author": "Chris Eppstein <chris@eppsteins.net> (http://chriseppstein.github.com)",
  "contributors": [
    "Jason Karns <jason@karns.name> (http://jason.karns.name)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nodenv/nodenv-each.git"
  },
  "bugs": {
    "url": "https://github.com/nodenv/nodenv-each/issues"
  },
  "directories": {
    "bin": "bin",
    "lib": "libexec",
    "test": "test"
  },
  "scripts": {
    "posttest": "npm run lint",
    "lint": "shellcheck $(git ls-files | grep -E -e '^bin/' -e '\\.sh$' -e '\\.bash$')",
    "publish:brew": "brew-publish $npm_package_name v$npm_package_version",
    "postversion": "git push --follow-tags && npm run publish:brew"
  },
  "devDependencies": {
    "brew-publish": "^2.3.1"
  }
}
